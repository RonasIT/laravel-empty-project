{
  "preset": "laravel",
  "rules": {
    "not_operator_with_successor_space": false,
    "concat_space": {
      "spacing": "one"
    },
    "class_attributes_separation": {
      "elements": {
        "method": "one"
      }
    },
    "array_syntax": {
      "syntax": "short"
    },
    "list_syntax": {
      "syntax": "long"
    }
  },
  "exclude": [
    "tests/fixtures/"
  ]
}