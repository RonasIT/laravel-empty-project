{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4",
    "doctrine/dbal": "^2.10",
    "facade/ignition": "^2.3.6",
    "fruitcake/laravel-cors": "^3.0",
    "laravel/framework": "^8.0",
    "laravel/legacy-factories": "^1.1",
    "laravel/tinker": "^2.4",
    "laravel/ui": "^3.0",
    "nunomaduro/collision": "^5.0",
    "ronasit/laravel-helpers": "2.0.3-beta",
    "superbalist/laravel-google-cloud-storage": "^2.2",
    "nikic/php-parser": "^4.12",
    "ronasit/laravel-swagger": ">=2.0.4-beta"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.7",
    "brainmaestro/composer-git-hooks": "^2.8",
    "fakerphp/faker": "^1.19",
    "friendsofphp/php-cs-fixer": "^3.8",
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": "^9.5.21",
    "ronasit/laravel-entity-generator": ">=1.3.4",
    "tightenco/tlint": "^6.3"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "[ $COMPOSER_DEV_MODE -eq 0 ] || php artisan ide-helper:generate",
      "[ $COMPOSER_DEV_MODE -eq 0 ] || php artisan ide-helper:meta",
      "[ $COMPOSER_DEV_MODE -eq 0 ] || php artisan ide-helper:models -N",
      "[ $COMPOSER_DEV_MODE -eq 0 ] || cghooks add --ignore-lock"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:models -N",
      "cghooks update"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "hooks": {
      "config": {
        "stop-on-failure": ["pre-commit"]
      },
      "pre-commit": [
        "docker-compose up -d php && docker-compose exec -T php vendor/bin/tlint"
      ]
    }
  },
  "minimum-stability": "beta"
}
